<h1>Doctor: <%= @doctor.name %></h1>
<h2>Specialty: <%= @doctor.specialty %></h2>
<h2>University Attended: <%= @doctor.university %></h2>
<h2>Hospital Attending: <%= @doctor.hospital_name %>

<h3>Patients: 
<ul>
<% @doctor.patients.each do |patient| %>
  <li id="patient_<%=patient.id %>">
    <%= patient.name %>
    <%= button_to "Remove Patient", doctor_patient_path(@doctor.find_join_id(patient)), method: :delete %>
  </li>
    <br>
<% end %>
</ul>
